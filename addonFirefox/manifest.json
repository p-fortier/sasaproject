{

  "manifest_version": 2,
  "name": "HideMyPrint",
  "version": "1.0",

  "description": "fakeheaders",

  "icons": {
    "48": "icons/border-48.png"
  },

  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "notifications",
    "http://localhost:3000/"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["requestHandler.js"]
    }
  ]
}
